    <tools name="MainToolbar">
        <tool name="AjaxSpellCheck"/>
        <tool name="FindAndReplace" shortcut="CTRL+F"/>
        <tool name="PasteStrip"/>
		<tool separator="true"/>
        <tool name="Undo" shortcut="CTRL+Z"/>
        <tool name="Redo" shortcut="CTRL+Y"/>
		
    </tools>

    <tools name="InsertToolbar" >
        <EditorToolStrip Name="InsertOptions" PopupHeight="250px">
            <EditorTool Name="ImageManager" Text="Insert Media" />
            <EditorTool Name="DocumentManager" />
            <EditorTool Name="FlashManager" />
            <EditorTool Name="MediaManager" />
            <EditorTool Name="ImageMapDialog"/>
        </EditorToolStrip>
        <EditorToolStrip Name="TemplateOptions" PopupHeight="80px">
            <EditorTool Name="SaveTemplate" Text="Save Template" />
            <EditorTool Name="TemplateManager" />
        </EditorToolStrip>
        <tool separator="true"/>
        <tool name="LinkManager" shortcut="CTRL+K"/>
        <tool name="Unlink" shortcut="CTRL+SHIFT+K"/>
        <tool name="InsertCustomLink" shortcut="CTRL+ALT+K"/>
    </tools>

    <tools name="DropdownToolbar" >
        <tool name="InsertSymbol"/>
        <tool name="InsertTable"/>
        <tool separator="true"/>
        <tool name="InsertParagraph" />
        <tool separator="true"/>
        <tool name="ToggleScreenMode" shortcut="F11"/>
        <tool separator="true"/>
        <tool name="InsertDate" />
        <tool name="InsertTime" />
    </tools>

    <tools>
        <tool name="Bold" shortcut="CTRL+B"/>
        <tool name="Italic" shortcut="CTRL+I"/>
        <tool name="Underline" shortcut="CTRL+U"/>
        <tool name="StrikeThrough" />
